{% extends "profile.html.twig" %}

{% block content %}
  <div class="text-right mb-2">
    <div id="month-picker" class="d-inline-block" data-default-value="{{ month|date('Y-m') }}"></div>
  </div>
  <div id="cubejs"
    data-api-url="{{ cubejs_api_url }}"
    data-token="{{ cube_token }}"
    data-query="{{ query|json_encode|e('html_attr') }}"
    class="my-2"></div>
  <form method="post">
    <button type="submit" class="btn btn-success">{{ 'basics.export'|trans }}</button>
  </form>
{% endblock %}

{% block styles %}
  {{ encore_entry_link_tags('metrics-loopeat') }}
{% endblock %}

{% block scripts %}
  {{ encore_entry_script_tags('metrics-loopeat') }}
{% endblock %}

